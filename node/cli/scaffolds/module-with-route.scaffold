'use strict';

module.exports = angular.module('${moduleName}', [])

  .controller('${controllerName}', require('./controllers/${controllerFile}'))

  .config(function ${moduleName}Config($routeProvider) {

    $routeProvider
      .when('/${moduleName}', {
        controller: '${controllerName}',
        controllerAs: '${moduleName}',
        templateUrl: 'templates/${moduleDashCase}.html',
        pageTitle: '${moduleTitleCase}',
        pageIcon: null
      });

  });